mixin  contact(data, options)
   .contact(class= data.class)
       - var id = data.id || 'new';
        .number
            .textnumber #{data.pos}
        .textInfo
            .name
                +input({place:'Name', type: 'text', value:data.name, name:'name_'+id})
            .pnumber
                +input({place:'Number', type: 'text', value:data.number, name:'number_'+id})
       +input({place:'', type: 'hidden', value:id, name:'id_'+id})
