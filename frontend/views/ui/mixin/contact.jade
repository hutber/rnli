mixin  contact(data, options)
   .contact(class= data.class)
       - c(data);
        .number
            .textnumber #{data.pos}
        .textInfo
            .name
                +input({place:'Name', type: 'text', value:data.name, name:'name_'+data.id})
            .pnumber
                +input({place:'Number', type: 'text', value:data.number, name:'number_'+data.id})
       +input({place:'', type: 'hidden', value:data.id, name:'id_'+data.id})
