mixin catchFish(data)
   - c(data.data);
   - data.attr = data.attr || {}
   .box.catchfishbox&attributes(data.attr)
      .image
         img(src='img/about.jpg')
      .text
         h3 #{data.data.species}
         .item W
            span #{data.data.weight1}lb #{data.data.weight2}oz
         .item L
            span #{data.data.height1}ft #{data.data.height2}in
